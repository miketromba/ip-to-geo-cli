{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node LTS",
    "_version": "18.1.0",
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "dist",
      "lib": ["es2023"],
      "module": "node16",
      "target": "es2022",
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16"
    }
  }